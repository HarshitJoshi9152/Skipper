{
    "manifest_version":2,
    "name": "Skipper",
    "description": "Meant to skip youtube ads on pressing `s` key, kinda like netflix !",
    "author": "harshit Joshi",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "arataka.jpeg",
        "default_title": "eris",
        "default_popup": "popup/pop.html"
    }

}
